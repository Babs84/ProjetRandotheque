{% extends 'Comptes/login.twig' %}

{% block contentSB %}
	<div id="snackbar">{{snackbartitle}}</div>
	<script>
	window.onload = snackbar();
	function snackbar() {
		if (start != 1) {
		    var x = document.getElementById("snackbar")
		    x.className = "show";
		    setTimeout(function(){ x.className = x.className.replace("show", ""); }, 3000);
		    var start = 1;
		}
	}
	</script>
{% endblock %}